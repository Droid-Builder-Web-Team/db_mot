{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.0",
        "acaronlex/laravel-calendar": "^1.3",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "barryvdh/laravel-snappy": "^0.4.8",
        "cache/filesystem-adapter": "^1.0",
        "coreui/coreui": "^3.2",
        "dialect/laravel-gdpr-compliance": "^1.4",
        "fideloper/proxy": "^4.2",
        "fortawesome/font-awesome": "^5.14",
        "fruitcake/laravel-cors": "^2.0",
        "glorand/laravel-model-settings": "^4.0",
        "google/apiclient": "^2.7",
        "guzzlehttp/guzzle": "^7.3",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "intervention/image": "^2.5",
        "laravel/framework": "^8.0",
        "laravel/sail": "^1.8",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.1",
        "league/flysystem-aws-s3-v3": "~1.0",
        "marvinlabs/laravel-discord-logger": "^1.1",
        "misd/linkify": "^1.1",
        "orangehill/iseed": "^2.6",
        "owen-it/laravel-auditing": "^12.0",
        "rap2hpoutre/laravel-log-viewer": "^1.7",
        "rennokki/rating": "^2.4",
        "silviolleite/laravelpwa": "^2.0",
        "spatie/calendar-links": "^1.4",
        "spatie/laravel-backup": "^6.16",
        "spatie/laravel-cookie-consent": "^2.12",
        "spatie/laravel-permission": "^3.13",
        "srmklive/paypal": "~1.0",
        "yajra/laravel-datatables-buttons": "^4.9",
        "yajra/laravel-datatables-html": "^4.30",
        "yajra/laravel-datatables-oracle": "^9.10",
        "yoeunes/toastr": "^1.2"
    },
    "require-dev": {
        "facade/ignition": "^2.11",
        "fzaninotto/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^4.4",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
